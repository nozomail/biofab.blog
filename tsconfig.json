{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "jsx": "preserve",
    "lib": ["DOM", "ESNext", "ESNext.Array"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false
  },
  "exclude": ["node_modules", "public", ".cache"]
}
